{
  "name": "@snipswap/surreal",
  "version": "1.0.0",
  "description": "",
  "main": "dist/index.cjs",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsup src/index.ts --format esm,cjs --dts --sourcemap",
    "build:scripts": "tsup --entry src/scripts/backup.ts --entry src/scripts/import.ts --entry src/scripts/restore.ts --outDir scripts --format esm",
    "watch": "pnpm build --watch",
    "watch:scripts": "pnpm build:scripts --watch",
    "import-all": "node ./scripts/import.mjs . -r",
    "import": "node ./scripts/import.mjs",
    "backup": "node ./scripts/backup.mjs",
    "restore": "node ./scripts/restore.mjs"
  },
  "keywords": [],
  "author": "Eddie McConkie",
  "license": "ISC",
  "type": "module",
  "dependencies": {
    "axios": "^1.4.0",
    "dotenv": "^16.3.1",
    "zod": "^3.21.4"
  },
  "devDependencies": {
    "tsup": "^7.1.0",
    "typescript": "^5.1.6"
  }
}
